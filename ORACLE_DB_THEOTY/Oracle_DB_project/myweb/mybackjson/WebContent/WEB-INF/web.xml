  </servlet>
  <servlet-mapping>
    <servlet-name>ProductListServlet</servlet-name>
    <url-pattern>/productlist</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>JacksonServlet</display-name>
    <servlet-name>JacksonServlet</servlet-name>
    <servlet-class>com.day.control.JacksonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JacksonServlet</servlet-name>
    <url-pattern>/jackson</url-pattern>
  </servlet-mapping>
